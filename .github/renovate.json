{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    "docker:pinDigests",
    "helpers:pinGitHubActionDigests",
    "group:allNonMajor",
    "npm:unpublishSafe",
    ":disableDependencyDashboard"
  ],
  "prCreation": "not-pending",
  "packageRules": [
    {
      "groupName": "@ui5/webcomponents packages",
      "matchPackageNames": [
        "@ui5/webcomponents{/,}**"
      ]
    }
  ]
}
