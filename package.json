    "@types/dagre": "^0.7.53",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.13.5",
    "@types/react": "^19.0.10",
    "@types/react-dom": "19.1.6",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@ui5/webcomponents-cypress-commands": "^2.7.2",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/eslint-plugin": "^1.1.37",
