{
  "name": "ui-frontend",
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "^24.0.0",
    "npm": "^11.0.0"
  },
  "scripts": {
    "dev": "tsx ./server.ts --local-dev",
    "start": "node dist/server.js",
    "build": "npm run build:server && npm run build:client",
    "build:client": "vite build",
    "build:server": "tsc -p tsconfig.server.json",
    "lint": "eslint ./src --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint ./src --fix",
    "preview": "vite preview",
    "test:vi": "vitest",
    "test:cy": "cypress run --component --browser chrome",
    "test:cy:open": "cypress open --component --browser chrome",
    "generate-graphql-types": "graphql-codegen --config graphql.config.yaml",
    "generate-graphql-types:watch": "graphql-codegen --config graphql.config.yaml --watch"
  },
  "dependencies": {
    "@apollo/client": "3.14.0",
    "@fastify/autoload": "6.3.1",
    "@fastify/cookie": "11.0.2",
    "@fastify/env": "5.0.3",
    "@fastify/helmet": "13.0.2",
    "@fastify/http-proxy": "11.3.0",
    "@fastify/secure-session": "8.2.0",
    "@fastify/sensible": "6.0.3",
    "@fastify/session": "11.1.0",
    "@fastify/static": "8.2.0",
    "@fastify/vite": "8.2.0",
    "@hookform/resolvers": "5.2.2",
    "@monaco-editor/react": "4.7.0",
    "@sentry/node": "10.17.0",
    "@sentry/react": "10.17.0",
    "@sentry/vite-plugin": "4.3.0",
    "@ui5/webcomponents": "2.15.0",
    "@ui5/webcomponents-fiori": "2.15.0",
    "@ui5/webcomponents-icons": "2.15.0",
    "@ui5/webcomponents-react": "2.15.0",
    "@ui5/webcomponents-react-charts": "2.15.0",
    "@xyflow/react": "12.8.6",
    "clsx": "2.1.1",
    "dagre": "0.8.5",
    "diff": "^8.0.2",
    "dotenv": "17.2.3",
    "fastify": "5.6.1",
    "fastify-plugin": "5.1.0",
    "graphql": "16.11.0",
    "graphql-config": "5.1.5",
    "i18next": "25.5.3",
    "javascript-time-ago": "2.5.12",
    "js-yaml": "4.1.0",
    "monaco-editor": "0.54.0",
    "monaco-yaml": "5.4.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-error-boundary": "6.0.0",
    "react-hook-form": "7.64.0",
    "react-i18next": "16.0.0",
    "react-router-dom": "7.9.3",
    "react-time-ago": "7.3.5",
    "swr": "2.3.6",
    "yaml": "2.8.1",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.37.0",
    "@graphql-codegen/cli": "6.0.0",
    "@graphql-codegen/client-preset": "5.1.0",
    "@testing-library/react": "16.3.0",
    "@types/dagre": "0.7.53",
    "@types/js-yaml": "4.0.9",
    "@types/node": "22.18.8",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.1",
    "@ui5/webcomponents-cypress-commands": "2.15.0",
    "@vitejs/plugin-react": "5.0.4",
    "@vitest/eslint-plugin": "1.3.16",
    "cypress": "15.3.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-i18next": "6.1.3",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jest-dom": "5.5.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "fastify-tsconfig": "3.0.0",
    "globals": "16.4.0",
    "jsdom": "27.0.0",
    "prettier": "3.6.2",
    "tsx": "4.20.6",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.0",
    "vite": "7.1.9",
    "vite-plugin-static-copy": "^3.1.2",
    "vitest": "3.2.4"
  }
}
