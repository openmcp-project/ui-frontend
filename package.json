{
  "name": "ui-frontend",
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "^24.0.0",
    "npm": "^11.0.0"
  },
  "scripts": {
    "dev": "tsx ./server.ts --local-dev",
    "start": "node dist/server.js",
    "build": "npm run build:server && npm run build:client",
    "build:client": "vite build",
    "build:server": "tsc -p tsconfig.server.json",
    "lint": "npm run lint:eslint && npm run lint:package-json",
    "lint:eslint": "eslint ./src --report-unused-disable-directives --max-warnings 0",
    "lint:eslint:fix": "eslint ./src --fix",
    "lint:package-json": "npmPkgJsonLint .",
    "preview": "vite preview",
    "test:vi": "vitest",
    "test:cy": "cypress run --component --browser chrome",
    "test:cy:open": "cypress open --component --browser chrome",
    "generate-graphql-types": "graphql-codegen --config graphql.config.yaml",
    "generate-graphql-types:watch": "graphql-codegen --config graphql.config.yaml --watch",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@apollo/client": "4.1.6",
    "@fastify/autoload": "6.3.1",
    "@fastify/cookie": "11.0.2",
    "@fastify/cors": "11.2.0",
    "@fastify/env": "5.0.3",
    "@fastify/helmet": "13.0.2",
    "@fastify/http-proxy": "11.4.1",
    "@fastify/secure-session": "8.3.0",
    "@fastify/sensible": "6.0.4",
    "@fastify/session": "11.1.1",
    "@fastify/static": "8.3.0",
    "@fastify/vite": "8.4.1",
    "@hookform/resolvers": "5.2.2",
    "@monaco-editor/react": "4.7.0",
    "@openapi-contrib/openapi-schema-to-json-schema": "5.1.0",
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/exporter-metrics-otlp-proto": "0.212.0",
    "@opentelemetry/exporter-trace-otlp-proto": "0.212.0",
    "@opentelemetry/instrumentation": "0.212.0",
    "@opentelemetry/instrumentation-fastify": "0.56.0",
    "@opentelemetry/instrumentation-undici": "0.22.0",
    "@opentelemetry/resources": "1.30.1",
    "@opentelemetry/sdk-metrics": "1.30.1",
    "@opentelemetry/sdk-trace-base": "1.30.1",
    "@opentelemetry/sdk-trace-node": "1.30.1",
    "@opentelemetry/semantic-conventions": "1.39.0",
    "@sentry/node": "10.40.0",
    "@sentry/react": "10.40.0",
    "@sentry/vite-plugin": "4.9.1",
    "@ui5/webcomponents": "2.19.2",
    "@ui5/webcomponents-fiori": "2.19.2",
    "@ui5/webcomponents-icons": "2.19.2",
    "@ui5/webcomponents-react": "2.19.0",
    "@ui5/webcomponents-react-charts": "2.19.0",
    "@xyflow/react": "12.10.1",
    "clsx": "2.1.1",
    "dagre": "0.8.5",
    "diff": "8.0.3",
    "dotenv": "17.3.1",
    "fastify": "5.7.4",
    "fastify-plugin": "5.1.0",
    "graphql": "16.13.0",
    "graphql-config": "5.1.5",
    "i18next": "25.8.13",
    "javascript-time-ago": "2.6.2",
    "js-yaml": "4.1.1",
    "monaco-editor": "0.55.1",
    "monaco-yaml": "5.4.1",
    "path-browserify": "1.0.1",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-hook-form": "7.71.2",
    "react-i18next": "16.5.4",
    "react-router-dom": "7.13.1",
    "react-time-ago": "7.4.1",
    "swr": "2.4.0",
    "yaml": "2.8.2",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.4",
    "@eslint/js": "9.39.3",
    "@graphql-codegen/cli": "6.1.2",
    "@graphql-codegen/client-preset": "5.2.3",
    "@testing-library/react": "16.3.2",
    "@types/dagre": "0.7.53",
    "@types/js-yaml": "4.0.9",
    "@types/node": "24.10.14",
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3",
    "@ui5/webcomponents-cypress-commands": "2.19.0",
    "@vitejs/plugin-react": "5.1.4",
    "@vitest/eslint-plugin": "1.6.9",
    "cypress": "15.11.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-i18next": "6.1.3",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jest-dom": "5.5.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.5.5",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "fastify-tsconfig": "3.0.0",
    "globals": "16.5.0",
    "jsdom": "27.4.0",
    "npm-package-json-lint": "9.1.0",
    "prettier": "3.8.1",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.56.1",
    "vite": "7.3.1",
    "vite-plugin-static-copy": "3.2.0",
    "vitest": "4.0.18"
  },
  "npmpackagejsonlint": {
    "rules": {
      "prefer-absolute-version-dependencies": "error",
      "prefer-absolute-version-devDependencies": "error"
    }
  }
}
